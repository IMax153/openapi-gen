{
  "name": "openapi-gen",
  "version": "0.0.0",
  "description": "Generate Effect http clients from OpenAPI specs",
  "packageManager": "pnpm@9.10.0",
  "scripts": {
    "version": "changeset version",
    "release": "pnpm run build && changeset publish",
    "test": "pnpm coverage",
    "clean": "rm -rf dist/*",
    "build": "tsup && pnpm copy-package-json",
    "build:ts": "tsup",
    "copy-package-json": "tsx scripts/copy-package-json.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tim-smart/openapi-gen.git"
  },
  "keywords": [],
  "author": "Tim Smart <hello@timsmart.co>",
  "license": "MIT",
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.8",
    "@effect/cli": "https://pkg.pr.new/Effect-TS/effect/@effect/cli@e2ff4b9",
    "@effect/platform": "https://pkg.pr.new/Effect-TS/effect/@effect/platform@e2ff4b9",
    "@effect/platform-node": "https://pkg.pr.new/Effect-TS/effect/@effect/platform-node@e2ff4b9",
    "@effect/schema": "https://pkg.pr.new/Effect-TS/effect/@effect/schema@e2ff4b9",
    "@types/node": "^22.5.4",
    "effect": "https://pkg.pr.new/Effect-TS/effect@e2ff4b9",
    "prettier": "^3.3.3",
    "tsup": "^8.2.4",
    "tsx": "^4.19.0",
    "typescript": "^5.6.2"
  }
}
